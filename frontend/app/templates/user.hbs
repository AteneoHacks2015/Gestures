{{outlet}}

<div class="container-full">
<div class="row">
	<div class="col-md-4 sidebar">
			<h2 class = "progress-decor">Level 1</h2>
			<div class="progress progress-striped active">
			   <div class="progress-bar progress-bar-success" role="progressbar" 
			      aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" 
			      style="width: 40%;">
			      <span class="sr-only">40% Complete</span>
			   </div>
			</div>
			<p id = "underneath-progress-bar" class = "progress-decor">10 more points to Level 2!</p>
	</div>
	<div class="col-md-8 dash">
		<h1 class = "dash">Welcome back, Name!</h1>
		<h2>Last played: Stage 2</h2>
		<button type="button" class="btn btn-default dash">Continue</button>
	</div>
</div>
</div>